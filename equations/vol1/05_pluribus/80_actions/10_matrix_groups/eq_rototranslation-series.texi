\begin{equation*}{}
  \begin{aligned}
\mgact(\tupp{\matmor{R}_{\monela}, \vectmor{t}_{\monelb}}, \mgact(\tupp{\matmor{R}_{\monelb}, \vectmor{t}_{\monelb}}, \vectob{p})) &=
\mgact(\tupp{\matmor{R}_{\monela}, \vectmor{t}_{\monela}}, \matmor{R}_{\monelb}\vectob{p} + \vectmor{t}_{\monelb}) \\
