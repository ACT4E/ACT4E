all:

chapter-once:
	latexmk -r ../../../../latexmkrc -g  -nobibtex standalone.tex

chapter-continuous:
	latexmk -r ../../../../latexmkrc -pvc -nobibtex standalone.tex

part-once:
	latexmk -r ../../../latexmkrc -pvc -nobibtex part-standalone.tex

part-continuous:
	latexmk -r ../../../latexmkrc -pvc -nobibtex part-standalone.tex
