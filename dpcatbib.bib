@article{heisenberg88reafferent,
    title = {Reafferent control of optomotor yaw torque in {\em Drosophila melanogaster}},
    author = {Heisenberg, M. and Wolf, R.},
    journal = {Journal of Comparative Physiology A: Neuroethology, Sensory, Neural, and Behavioral Physiology},
    year = {1988},
    month = may,
    number = {3},
    pages = {373--388},
    volume = {163},
    abstract = {In the flight simulator the optomotor response ofDrosophila melanogaster does not operate as a simple feedback loop. Reafferent and exafferent motion stimuli are processed differently. Under open-loop conditions responses to motion are weaker than under closed-loop conditions. It takes the fly less than 100 ms to distinguish reafferent from exafferent motion. In closed-loop conditions, flies constantly generate torque fluctuations leading to small-angle oscillations of the panorama. This reafferent motion stimulus facilitates the response to exafferent motion but does not itself elicit optomotor responses. Reafference control appears to be directionally selective: while a displacement of the patternm by as little as 0.1� against the �expected� direction leads to a fast syndirectional torque response, displacements in the �expected� direction have no comparable effect. Based on the behavior of the mutantrol sol, which under open-loop conditions is directionally motion-blind but in closed-loop conditions still performs optomotor balance, a model is proposed in which the fly's endogenous torque fluctuations are an essential part of the course control process. It is argued that the model may also account for wild type optomotor balance in the flight simulator.},
    doi = {10.1007/BF00604013},
    timestamp = {2011.02.01}
}

@article{hendrickx99critique,
    title = {A Critique of Leyton's Theory of Perception and Cognition. Review of Symmetry, Causality, Mind, by Michael Leyton },
    author = {Marcel Hendrickx and Johan Wagemans},
    journal = {Journal of Mathematical Psychology },
    year = {1999},
    number = {2},
    pages = {314 - 345},
    volume = {43},
    abstract = {Michael Leyton is on the faculty of the Psychology Department at Rutgers University. He received his Ph.D. from the University of California at Berkeley in 1984. He is a recipient of the Presidential Young Investigator Award for outstanding work in cognitive science. Symmetry, Causality, Mind represents the synthesis of a theory that goes back to his Ph.D. thesis (1984a) and that was gradually developed in a number of papers, totalling more than 350 pages, published in journals like Biological Cybernetics (1984b); Computer Vision, Graphics and Image Processing (1985, 1987a, 1987b); Information Sciences (1986a); Journal of Mathematical Psychology (1986b, 1986c, 1987c); Artificial Intelligence (1988); and Cognitive Science (1989). The entire first issue of the 1986 volume (Vol. 38) of Information Sciences, comprising some 120 pages, was devoted to his theory. Marcel Hendrickx has studied mathematics, computer science, and library and information science at different universities in Belgium. In 1995, he completed the Master of Artificial Intelligence program at the University of Leuven where he did his M.Sc. thesis on Leyton's work under the supervision of Johan Wagemans. In 1997, he also completed the Master of Statistics program. He has more than 10 years of experience as a software developer in several software companies, at the government, and at the Department of Computer Vision at the University of Leuven. Johan Wagemans is an associate professor in the Department of Psychology at the University of Leuven. His primary research interests are visual perception, particularly perceptual organization (symmetry, grouping) and shape perception (contour curvature, invariants). He has published a large number of journal articles and book chapters in these areas. He also has broad interests and teaching experience in cognitive science and artificial intelligence. },
    doi = {10.1006/jmps.1998.1232},
    issn = {0022-2496}
}

