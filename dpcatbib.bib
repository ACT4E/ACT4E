@article{bongard06resilient,
    title = {{Resilient Machines Through Continuous Self-Modeling}},
    author = {Bongard, Josh and Zykov, Victor and Lipson, Hod},
    journal = {Science},
    year = {2006},
    month = nov,
    number = {5802},
    pages = {1118--1121},
    volume = {314},
    abstract = {{Animals sustain the ability to operate after injury by creating qualitatively different compensatory behaviors. Although such robustness would be desirable in engineered systems, most machines fail in the face of unexpected damage. We describe a robot that can recover from such change autonomously, through continuous self-modeling. A four-legged machine uses actuation-sensation relationships to indirectly infer its own structure, and it then uses this self-model to generate forward locomotion. When a leg part is removed, it adapts the self-models, leading to the generation of alternative gaits. This concept may help develop more robust machines and shed light on self-modeling in animals. 10.1126/science.1133687}},
    day = {17},
    doi = {10.1126/science.1133687},
